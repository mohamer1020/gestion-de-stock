{
  "version": 2,
  "builds": [
    { "src": "**/*.php", "use": "vercel-php@0.7.0" },
    { "src": "**/*.css", "use": "@vercel/static" },
    { "src": "**/*.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/(.*).php", "dest": "/$1.php" },
    { "src": "/", "dest": "/index.php" }
  ]
}
