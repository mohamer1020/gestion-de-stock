{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.0"
    },
    "*.php": {
      "runtime": "vercel-php@0.7.0"
    }
  },
  "routes": [
    { "src": "/(.*).css", "dest": "/$1.css" },
    { "src": "/(.*).js", "dest": "/$1.js" },
    { "src": "/(.*).png", "dest": "/$1.png" },
    { "src": "/(.*).jpg", "dest": "/$1.jpg" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
