{
  "builds": [
    {
      "src": "*.php",
      "use": "vercel-php@0.7.0"
    },
    {
      "src": "css/*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/(.*).css", "dest": "/$1.css" },
    { "src": "/(.*).php", "dest": "/$1.php" },
    { "src": "/", "dest": "/index.php" }
  ]
}
